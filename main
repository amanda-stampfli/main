#Sprint 2:
grade = 98
if grade >= 95:
    print("Very good.")
temp = int(input("Enter the temperature: "))
if temp >= 75:
    print("Yay!")
else:
    if temp >= 45:
        print("Too cold")
    else:
        print("Not so good.")
num = 45
if num > 0:
    print("Good number")
elif num == 0:
    print("Zero")
else:
    print("Not a good number")
money = 20.00
if money >=20:
    print("Good amount")
elif money <20:
    print("I'm broke")
drink = 22
if drink != 25:
    print("Not true")
else:
    print("True")
mountain = 0
while (mountain < 20):
    print("The mountain is:", mountain)
    mountain = mountain + 1
print("Goodbye!")
Dogs = ["Labrador", "Great Dane", "Husky"]
for x in Dogs:
    print(x)
for x in range(10,24,2):
    print(x, end=" ")
name = input("what is your dog's name: ")
for x in range(10):
    for x in range(7):
        print(name + " ", end=" ")
    print()
