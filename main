#Squared
x = 7
y = 2
print(x ** y)
#Cubed
tomato = 8
tamato = 3
print(tomato ** tamato)
#Multiplication
food = 23
water = 30
print(food * water)
#Division
Disney = 111
World = 11
print(Disney/World)
#Divide and round down
Naples = 112
Florida = 60
print(Naples // Florida)
#Addition
Laugh = 235
Love = 600
